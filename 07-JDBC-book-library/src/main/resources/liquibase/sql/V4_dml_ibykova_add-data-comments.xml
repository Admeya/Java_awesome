<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="irina" id="insert-comments">
        <insert tableName="comments">
            <column name="comment_id"  value="1"/>
            <column name="description" value="Very good book"/>
        </insert>
        <insert tableName="comments">
            <column name="comment_id"  value="2"/>
            <column name="description" value="Not so bad"/>
        </insert>
    </changeSet>
    <changeSet author="irina" id="insert-book-comments">
        <insert tableName="books_comments">
            <column name="book_id" value="1"/>
            <column name="comment_id" value="1"/>
        </insert>
        <insert tableName="books_comments">
            <column name="book_id" value="1"/>
            <column name="comment_id" value="2"/>
        </insert>
    </changeSet>
</databaseChangeLog>




